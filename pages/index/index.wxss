/**index.wxss**/

page {
  background: #fafafa;
}

.title_bar {
  top: 0;
  position: absolute;
  left: 0;
  width: 100%;
}

.Index-Head {
  width: 100%;
  height: 450rpx;
  padding-top: 150rpx;
  background: #29aff6;
  color: #fff;
  border-bottom-left-radius: 10%;
  border-bottom-right-radius: 10%;
}

.Index-Head .Index-Head-Title {
  display: block;
  font-size: 70rpx;
}

.Index-Head .Index-Head-Tips {
  font-size: 30rpx;
  letter-spacing: 15rpx;
}

.Index-Head-Search {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 80rpx;
}

.Index-Head-Search image {
  width: 36rpx;
  height: 48rpx;
  margin-right: 20rpx;
}

.Index-Head-Search .section {
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
}

.Index-Head-Search .section text {
  display: inline-block;
  max-width: 400rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.Index-body {
  width: 630rpx;
  height: 130rpx;
  padding: 85rpx 0;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 10rpx 20rpx #ddd;
  position: absolute;
  top: 480rpx;
  left: 60rpx;
  background: #34d65e;
}

.Index-body.arrow:after {
  position: absolute;
  top: 120rpx;
  right: 20rpx;
}

.Index-body image {
  width: 165rpx;
  height: 130rpx;
  position: absolute;
  top: 85rpx;
  left: 40rpx;
}

.Index-body .Index-body-Title {
  display: block;
  text-indent: 40rpx;
  font-size: 48rpx;
}

.Index-body .Index-body-Tips {
  display: block;
  text-indent: 120rpx;
  font-size: 30rpx;
  color: #999;
}

.Index-body-order {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0);
  z-index: 12;
}

.Index-Foot {
  margin-top: 300rpx;
}

.Index-Foot button {
  line-height: 80rpx;
  padding: 0rpx 45rpx;
  background: #fff;
  font-size: 36rpx;
  border: #e6e6e6 4rpx solid;
  border-radius: 50rpx;
}

.Index-Foot button::after {
  border: 0;
}

.mobile_model {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 13;
  background: rgba(0, 0, 0, 0.3);
}

.mobile_model .mobile_model_content {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #fff;
  width: 600rpx;
  border-radius: 18rpx;
  font-size: 26rpx;
}

.mobile_model_content > image {
  display: block;
  width: 80rpx;
  height: auto;
  margin-top: 50rpx;
  margin-bottom: 20rpx;
  border-radius: 50%;
  box-shadow: 2rpx 2rpx 5rpx #ccc;
}

.mobile_model_content > .content {
  width: 540rpx;
  height: auto;
}

.content > text {
  display: block;
  margin-bottom: 20rpx;
  font-size: 30rpx;
  text-align: center;
}

.content > .ipt_mobile {
  height: 80rpx;
  text-align: left;
  padding: 0 10rpx;
  margin-bottom: 20rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.content .code {
  width: 100%;
  display: flex;
  align-items: center;
  margin-bottom: 50rpx;
}

.content .code .ipt_code {
  flex: 1;
  height: 80rpx;
  padding: 0 10rpx;
  margin-right: 20rpx;
  text-align: left;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.content .code .send {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #fff;
  background: #29aff6;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.content .code .send[disabled] {
  background: #ccc;
}

.content .code .send::after {
  border: none;
}

.content .code .send[disabled]::after {
  border: none;
}

.mobile_model_content .btn_group {
  display: flex;
  width: 100%;
  border-top: 1rpx solid #ccc;
}

.btn_group > button {
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  background: #fff;
}

.btn_group > button::after {
  border: none;
  border-radius: 0;
}

.btn_group .cancel {
  border-right: 1rpx solid #ccc;
  border-bottom-left-radius: 18rpx;
}

.btn_group .confirm {
  color: #34d65e;
  border-bottom-right-radius: 18rpx;
}

